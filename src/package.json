{
  "name": "helasuno",
  "displayName": "Helasuno",
  "description": "Basic Helasuno language support for VS Code",
  "publisher": "vansmith",
  "author": {
    "name": "Bryan Smith"
  },
  "version": "0.0.1",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "helasuno",
      "aliases": ["Helasuno", "helasuno"],
      "extensions": [".hls"],
      "icon": {
        "light": "icon.png",
        "dark": "icon.png"
      },
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "helasuno",
      "scopeName": "source.helasuno",
      "path": "./syntaxes/helasuno.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "helasuno",
        "path": "./snippets/helasuno.code-snippets"
      }
    ]
  }
}
